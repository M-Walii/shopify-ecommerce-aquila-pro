<section class="site-footer">
  <div class="container">
    <nav aria-label="{{ 'sections.footer.navigation' | t }}">
      <ul>
        {% for link in linklists.footer.links %}
          <li><a href="{{ link.url }}">{{ link.title }}</a></li>
        {% endfor %}
      </ul>
    </nav>
    <p>&copy; {{ 'now' | date: '%Y' }} {{ shop.name }}</p>
  </div>
</section>

{% schema %}
{
  "name": "Footer",
  "settings": []
}
{% endschema %}
